<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.imooc.o2o.dao.LocalAuthDao">
    <insert id="insertLocalAuth" useGeneratedKeys="true" keyColumn="local_auth_id"  keyProperty="localAuthId">
    INSERT INTO
    tb_local_auth(user_name,password,create_time,last_edit_time)
    VALUES
    (#{userName},#{password},#{createTime},#{lastEditTime})
    </insert>
    </mapper>